# Object Class
objectclass ( 1.3.6.1.4.1.38414.39.2.1 NAME 'fdInventoryContent'
  DESC 'FusionDirectory inventory information'
  MUST ( cn )
  MAY ( macAddress ) )

objectclass ( 1.3.6.1.4.1.38414.39.2.2 NAME 'fdInventoryCpu'
  DESC 'FusionDirectory inventory information'
  MUST ( cn )
  MAY ( fdInventoryCore         $ fdInventoryName   $ fdInventoryFamilyNumber $
        fdInventoryManufacturer $ fdInventoryModel  $ fdInventorySpeed $
        fdInventoryStepping     $ fdInventoryThread ) )

objectclass ( 1.3.6.1.4.1.38414.39.2.3 NAME 'fdInventoryOperatingSystem'
  DESC 'FusionDirectory inventory information'
  MUST ( cn )
  MAY ( fdInventoryName       $ fdInventoryVersion        $ fdInventoryFullName $
        fdInventoryKernelName $ fdInventoryKernelVersion  ) )

objectclass ( 1.3.6.1.4.1.38414.39.2.4 NAME 'fdInventoryNetwork'
  DESC 'FusionDirectory inventory information'
  MUST ( cn )
  MAY ( fdInventoryDescription  $ fdInventoryIPAddress  $ fdInventoryIPMask   $
        fdInventoryIPSubnet     $ fdInventoryMacAddr    $ fdInventoryStatus   $
        fdInventoryVirtualDev   $ fdInventoryDriver     $ fdInventoryPCISlot  $
        fdInventoryIPAddress6   $ fdInventoryIPMask6    $ fdInventoryIPSubnet6) )
