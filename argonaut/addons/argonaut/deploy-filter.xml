<?xml version="1.0" encoding="UTF-8"?>

<filterdef>
  <definition>
    <category>argonautQueue</category>
    <initial>true</initial>
  </definition>

  <search>
    <query>
      <backend>ArgonautEvents</backend>
      <filter>(objectClass=*)</filter>
      <attribute>dn</attribute>
      <attribute>objectClass</attribute>
    </query>
    <scope>auto</scope>
  </search>
</filterdef>
