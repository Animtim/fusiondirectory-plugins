<?xml version="1.0" encoding="UTF-8"?>

<filterdef>
  <definition>
    <category>memberNisnetgroups</category>
    <template>../plugins/admin/netgroups/memberNisnetgroupSelect/memberNisnetgroup-filter.tpl</template>
    <initial>true</initial>
  </definition>

  <search>
    <query>
      <backend>MemberNisnetgroupLDAP</backend>
      <filter>(&amp;(objectClass=nisNetgroup)$NAME)</filter>
      <attribute>dn</attribute>
      <attribute>objectClass</attribute>
      <attribute>cn</attribute>
      <attribute>uid</attribute>
      <attribute>description</attribute>
    </query>
    <scope>auto</scope>
  </search>

  <element>
    <type>textfield</type>
    <tag>NAME</tag>
    <size>20</size>
    <maxlength>60</maxlength>
    <set>(cn=*$*)</set>
    <unset>(cn=*)</unset>
    <default></default>
    <alphabet>true</alphabet>
  </element>

</filterdef>
