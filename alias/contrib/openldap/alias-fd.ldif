dn: cn=alias-fd,cn=schema,cn=config
objectClass: olcSchemaConfig
cn: cn=alias-fd
##
## alias-fd.schema - Needed by Fusion Directory for managing aliases
##
# Attributes
olcAttributeTypes: ( 1.3.6.1.4.1.38414.11.1.1NAME 'aliasExpirationDate' DESC 'FusionDirectory - alias expiration date' EQUALITY integerMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE )
olcAttributeTypes: ( 1.3.6.1.4.1.38414.11.1.2NAME 'fdAliasRDN' DESC 'FusionDirectory - Alias RDN' EQUALITY caseExactIA5Match SUBSTR caseExactIA5SubstringsMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 SINGLE-VALUE)
# Object Class
olcObjectClasses: ( 1.3.6.1.4.1.38414.11.2.1NAME 'mailAliasRedirection' DESC 'FusionDirectory - Mail alias class' MUST ( cn $ mail $ gosaMailForwardingAddress ) MAY  ( description $ aliasExpirationDate) )
olcObjectClasses: ( 1.3.6.1.4.1.38414.11.2.2NAME 'mailAliasDistribution' DESC 'FusionDirectory - Mail alias class' MUST ( cn $ mail $ gosaMailAlternateAddress $ gosaMailServer ) MAY  ( description $ aliasExpirationDate) )
olcObjectClasses: ( 1.3.6.1.4.1.38414.11.2.3NAME 'fusionDirectoryAliasConf' DESC 'FusionDirectory alias plugin configuration' SUP top AUXILIARY MUST ( cn ) MAY ( fdAliasRDN ) )
