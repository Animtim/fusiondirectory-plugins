# Object Class

objectclass ( 1.3.6.1.4.1.38414.16.2.1 NAME 'fdMobilePhone'
  DESC 'FusionDirectory mobile phone class'
  MUST ( cn )
  MAY ( description $ telephoneNumber $ serialNumber $ fdMobileIMEI ) )

objectclass ( 1.3.6.1.4.1.38414.16.2.2 NAME 'fdSystemsPluginConf'
  DESC 'FusionDirectory systems plugin configuration'
  SUP top AUXILIARY
  MUST ( cn )
  MAY ( fdSystemRDN $ fdServerRDN $ fdWorkstationRDN $ fdTerminalRDN $
        fdPrinterRDN $ fdComponentRDN $ fdMobilePhoneRDN ) )
