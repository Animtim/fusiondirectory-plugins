objectClass ( 1.3.6.1.4.1.38414.52.2.1  NAME 'fdCommunityOrganization'
  DESC 'Community Organization'
  SUP top AUXILIARY
  MUST ( fdStartDate $ manager $ member )
  MAY ( fdMembershipType $ fdOrganizationAlternateLastName $ fdOrganizationAlternateFirstName $
		fdOrganizationAlternateStreet $ fdOrganizationAlternateZipCode $ fdOrganizationAlternateCity $
		fdOrganizationAlternateState $ fdOrganizationAlternateCountry $ fdEndDate))

objectClass ( 1.3.6.1.4.1.38414.52.2.2  NAME 'fdCommunityProject'
  DESC 'Community Project'
  SUP top AUXILIARY
  MUST ( fdStartDate $ manager $ member )
  MAY ( fdMembershipType $ fdOrganizationAlternateLastName $ fdOrganizationAlternateFirstName $
		fdOrganizationAlternateStreet $ fdOrganizationAlternateZipCode $ fdOrganizationAlternateCity $
		fdOrganizationAlternateState $ fdOrganizationAlternateCountry $ fdEndDate $ fdProjectKey  ))
