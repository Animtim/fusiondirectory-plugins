attributetype ( 1.3.6.1.4.1.38414.52.1.3 NAME 'fdOrganizationAlternateFirstName'
  DESC 'fdOrganization Alternate First Name'
  SUP gn )
  
attributetype ( 1.3.6.1.4.1.38414.52.1.4 NAME 'fdOrganizationAlternateStreet'
  DESC 'fdOrganization Alternate Street'
