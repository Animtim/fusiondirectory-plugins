olcAttributeTypes: ( 1.3.6.1.4.1.38414.12.1.1NAME 'sympaServerURL' DESC 'FusionDirectory - The sympa server url' EQUALITY caseIgnoreIA5Match SUBSTR caseIgnoreSubstringsMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 SINGLE-VALUE )
olcAttributeTypes: ( 1.3.6.1.4.1.38414.12.1.2NAME 'sympaServerUser' DESC 'FusionDirectory - The sympa server user' EQUALITY caseIgnoreIA5Match SUBSTR caseIgnoreSubstringsMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 SINGLE-VALUE )
olcAttributeTypes: ( 1.3.6.1.4.1.38414.12.1.3NAME 'sympaServerPassword' DESC 'FusionDirectory - The sympa server password' EQUALITY caseIgnoreIA5Match SUBSTR caseIgnoreSubstringsMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 SINGLE-VALUE )
