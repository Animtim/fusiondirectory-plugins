##
## ipmi-fd.schema - Needed by Fusion Directory for IPMI plugin
##

# Attributes
attributetype ( 1.3.6.1.4.1.38414.29.1.1 NAME 'fdIpmiLogin'
  DESC 'Fusion Directory - IPMI user login'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
  SINGLE-VALUE )

attributetype ( 1.3.6.1.4.1.38414.29.1.2 NAME 'fdIpmiPassword'
  DESC 'Fusion Directory - IPMI user password'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
  SINGLE-VALUE )

# Object Class
objectclass ( 1.3.6.1.4.1.38414.29.2.1 NAME 'ipmiInterface'
  DESC 'An IPMI client'
  SUP top AUXILIARY
  MUST ( cn )
  MAY  (  description $ macAddress      $ ipHostNumber $
          fdIpmiLogin $ fdIpmiPassword ) )

