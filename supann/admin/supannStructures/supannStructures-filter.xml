<?xml version="1.0" encoding="UTF-8"?>

<filterdef>
  <definition>
    <category>SupannStructures</category>
    <template>supannStructures-filter.tpl</template>
    <initial>true</initial>
  </definition>

  <search>
    <query>
      <backend>LDAP</backend>
      <filter>(&amp;(objectClass=supannEntite)$NAME)</filter>
      <attribute>dn</attribute>
      <attribute>objectClass</attribute>
      <attribute>ou</attribute>
      <attribute>description</attribute>
      <attribute>o</attribute>
      <attribute>supannCodeEntiteParent</attribute>
      <attribute>supannCodeEntite</attribute>
    </query>
    <scope>auto</scope>
  </search>

  <element>
    <type>textfield</type>
    <tag>NAME</tag>
    <size>18</size>
    <maxlength>60</maxlength>
    <default></default>
    <unset></unset>
    <set>(|(supannCodeEntite=*$*)(supannCodeEntiteParent=*$*)(supannEtablissement=*$*)(cn=*$*)(ou=*$*)(o=*$*))</set>
    <alphabet>false</alphabet>
    <autocomplete>
      <backend>LDAP</backend>
      <filter>(&amp;(objectClass=supannEntite)(|(supannCodeEntite=*$*)(supannCodeEntiteParent=*$*)(supannEtablissement=*$*)(cn=*$*)))</filter>
      <attribute>supannCodeEntite</attribute>
      <attribute>supannCodeEntiteParent</attribute>
      <attribute>supannEtablissement</attribute>
      <attribute>eduOrgLegalName</attribute>
      <attribute>o</attribute>
      <attribute>cn</attribute>
      <frequency>0.5</frequency>
      <characters>3</characters>
    </autocomplete>
  </element>

</filterdef>
